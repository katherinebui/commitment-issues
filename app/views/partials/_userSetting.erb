<form action="/users/<%= @user.id %>" method="POST" id="phone_time_form">
  <input type="hidden" name="_method" value="PUT">
  <input type="hidden" name=<%= @user.username%> >
  <input type="hidden" name=<%= @user.password%> >
  <input type="hidden" name=<%= @user.email%> >
  <input type="text" name="user[phone_number]" placeholder="Can I get yo digits?">
  <input type="text" name="user[time_increment]" placeholder="Timer">
  <input type="submit" value="Submit" id="pp-button">
</form>
